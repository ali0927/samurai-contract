    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "^4.4.1",
    "@openzeppelin/contracts-upgradeable": "^4.4.1",
    "@openzeppelin/hardhat-upgrades": "^1.12.0",
    "@openzeppelin/test-helpers": "^0.5.15",
    "@typechain/ethers-v5": "^5.0.0",
    "@typechain/hardhat": "^1.0.1",
    "@types/chai": "^4.2.13",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.11.8",
    "axios": "^0.24.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "hardhat": "^2.8.0",
    "hardhat-deploy": "^0.9.24",
    "hardhat-gas-reporter": "^1.0.6",
    "hardhat-typechain": "^0.3.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^8.10.2",
    "typechain": "^4.0.1",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "deploy": "npx hardhat run --network mainnet scripts/deploy.js",
    "paramCheck": "npx hardhat run --network mainnet utils/paramCheck.js",
    "whitelistUsers": "npx hardhat run --network mainnet scripts/whitelistUsers.js",
    "devMint": "npx hardhat run --network mainnet scripts/devMint.js",
    "pulse": "npx hardhat run --network mainnet scripts/pulse.js",
    "airdrop": "npx hardhat run --network mainnet scripts/airdrop.js",
    "updateBaseUri": "npx hardhat run --network mainnet scripts/updateBaseURI.js",
    "reveal": "npx hardhat run --network mainnet scripts/reveal.js",
    "refreshMetadata": "node utils/refreshMetadata.js",
    "setPresaleOpen": "npx hardhat run --network mainnet utils/setPresaleOpen.js",
    "setPublicSaleOpen": "npx hardhat run --network mainnet utils/setPublicSaleOpen.js",
    "createFakeWallets": "node utils/createFakeWallets.js",
    "snapshot": "npx hardhat run --network hardhat utils/snapShot.js",
    "start-shogun": "npx hardhat node --tags ShogunNFT",
    "start-sacrifice": "npx hardhat node --tags ShogunNFT,Sacrifice",
    "start-staking": "npx hardhat node --tags ShogunNFT,ShogunStaking,ShoToken",
    "test-sacrifice": "npx hardhat test --network localhost ./test/Sacrifice.test.js",
    "deploy-sacrifice-rinkeby": "npx hardhat deploy --network rinkeby --tags Sacrifice",
    "deploy-sacrifice-mainnet": "npx hardhat deploy --network mainnet --tags Sacrifice",
    "test-staking": " npx hardhat test ./test/ShogunStaking.test.js",
    "verify-mainnet": "npx hardhat --network mainnet etherscan-verify",
    "verify-kovan": "npx hardhat --network kovan etherscan-verify",
    "verify-rinkeby": "npx hardhat --network rinkeby etherscan-verify",
    "verify-mumbai": "npx hardhat --network mumbai etherscan-verify"
  }
